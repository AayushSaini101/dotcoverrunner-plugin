<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry field="vsTestPlatform" title="Target platform">
        <select name="vsTestPlatform">
            <option value="x64">x64</option>
            <option value="x86">x86</option>
            <option value="ARM">ARM</option>
        </select>
    </f:entry>

    <f:entry field="vsTestCaseFilter" title="Test case filter" description="Test case filter passed through to VSTest.">
        <f:textbox default="${descriptor.DEFAULT_VSTESTCASEFILTER}"/>
    </f:entry>

    <f:entry field="vsTestAssemblyFilter"
             title="Glob that defines which test assemblies to generata coverage data for.">
        <f:textbox/>
    </f:entry>

    <f:entry field="vsTestArgs" title="Additional VSTest arguments to pass through.">
        <f:textbox/>
    </f:entry>

    <f:entry field="coverageInclude" title="Semicolon-separated list of assemblies to include in the coverage.">
        <f:textbox/>
    </f:entry>

    <f:entry field="coverageClassInclude" title="Semicolon-separated list of classes to include in the coverage.">
        <f:textbox/>
    </f:entry>

    <f:entry field="coverageFunctionInclude" title="Semicolon-separated list of methods to include in the coverage">
        <f:textbox/>
    </f:entry>

    <f:entry field="processInclude" title="Semicolon-separated list processes to regard as part of the coverage run.">
        <f:textbox/>
    </f:entry>

    <f:entry field="processExclude" title="Semicolon-separated list processes to disregard in the coverage run.">
        <f:textbox/>
    </f:entry>

    <f:entry field="htmlReportPath" title="Name of the HTML coverage report file.">
        <f:textbox/>
    </f:entry>

    <f:entry field="nDependXmlReportPath" title="Name of the nDepend XML coverage report file.">
        <f:textbox/>
    </f:entry>

    <f:entry field="detailedXMLReportPath" title="Name of the detailed XML coverage report file.">
        <f:textbox/>
    </f:entry>


</j:jelly>